{
  "name": "il2cpp-hooker",
  "version": "1.0.0",
  "description": "UnityApi Hook tools based on frida-il2cpp-bridge",
  "main": "agent/index.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "frida-compile agent/index.ts -c -o _Ufunc.js",
    "watch": "frida-compile agent/index.ts -w -o _Ufunc.js",
    "compress": "uglifyjs _Ufunc.js -m -c -o _Ufunc.js"
  },
  "devDependencies": {
    "@types/frida-gum": "^18.2.0",
    "@types/node": "^17.0.45",
    "@frida/http": "^4.0.2",
    "@frida/https": "^1.0.0"
  },
  "dependencies": {
    "babelify": "^10.0.0",
    "chalk": "^5.0.1",
    "decorator-cache-getter": "^1.0.0",
    "fastest-levenshtein": "^1.0.12",
    "frida-compile": "^10.2.5",
    "frida-il2cpp-bridge": "0.7.13",
    "numbro": "^2.3.6",
    "prettier": "^2.6.2",
    "uglifyjs": "^2.4.11",
    "versioning": "^1.1.2"
  }
}
